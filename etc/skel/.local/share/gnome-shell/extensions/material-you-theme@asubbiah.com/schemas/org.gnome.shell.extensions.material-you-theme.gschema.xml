<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema id="org.gnome.shell.extensions.material-you-theme" path="/org/gnome/shell/extensions/material-you-theme/">
        <key name="scheme" type="s">
            <default>"Default"</default>
            <summary>Current color scheme</summary>
        </key>
        <key name="show-notifications" type="b">
            <default>true</default>
            <summary>Show notifications on theme update</summary>
        </key>
        <key name="resize-width" type="i">
            <default>64</default>
            <summary>Width to resize wallpaper to</summary>
        </key>
        <key name="resize-height" type="i">
            <default>64</default>
            <summary>Height to resize wallpaper to</summary>
        </key>
    </schema>
</schemalist>